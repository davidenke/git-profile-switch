{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "org.davidenke.git-profile-switch",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": ["app.*", "os.*", "debug.log"],
  "modes": {
    "window": {
      "alwaysOnTop": true,
      "borderless": true,
      "enableInspector": true,
      "exitProcessOnClose": false,
      "hidden": false,
      "maximize": false,
      "resizable": false,
      "width": 400,
      "height": 200,
      "title": "Git Profile Switch",
      "icon": "/resources/icons/app.png"
    }
  },
  "cli": {
    "binaryName": "git-profile-switch",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/node_modules/@neutralinojs/lib/dist/neutralino.js",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2"
  }
}
