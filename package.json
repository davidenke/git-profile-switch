{
  "name": "@davidenke/git-profile-switch",
  "version": "0.0.1",
  "description": "Mac menu bar switch for git profiles.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "shx rm -rf dist",
    "prebuild": "run-s clean",
    "build:server": "rollup --config rollup.config.ts",
    "build:ui": "stencil build --docs",
    "build": "run-p build:*",
    "package": "electron-packager dist \"Git Profile Switch\" --out bundle --overwrite",
    "prestart": "run-s clean",
    "start:server": "rollup --config rollup.config.ts --watch",
    "start:ui": "stencil build --docs --dev --es5 --watch --serve",
    "start:preview:wait:ui": "wait-on http://localhost:3333/index.html",
    "start:preview:wait:server": "wait-on dist/main.js dist/preload.js",
    "start:preview:server": "electron dist",
    "start:preview": "run-s start:preview:wait:* start:preview:server",
    "start": "run-p start:*",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll"
  },
  "author": {
    "email": "david@enke.dev",
    "name": "David Enke"
  },
  "dependencies": {
    "electron-util": "0.14.2",
    "glob": "7.1.6",
    "jpeg-js": "0.4.3",
    "md5": "2.3.0",
    "mkdirp": "1.0.4",
    "node-fetch": "2.6.1",
    "pngjs": "6.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-typescript": "8.2.0",
    "@stencil/core": "2.4.0",
    "@stencil/postcss": "2.0.0",
    "@stencil/sass": "1.4.1",
    "@types/glob": "7.1.3",
    "@types/md5": "2.3.0",
    "@types/mkdirp": "1.0.1",
    "@types/node-fetch": "2.5.8",
    "@types/node": "14.14.31",
    "@types/pngjs": "6.0.0",
    "autoprefixer": "10.2.4",
    "electron": "12.0.0",
    "electron-packager": "15.2.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.40.0",
    "shx": "0.3.3",
    "tslib": "2.1.0",
    "typescript": "4.2.2",
    "wait-on": "5.2.1"
  },
  "license": "MIT"
}
